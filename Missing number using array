import java.util.*;
class Main {
    public static void main(String[] args) {
         Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int []arr = new int[n];
        for(int i=0;i<n-1;i++){
            arr[i]=sc.nextInt();
        }
       int sum=0;
        for(int i=0;i<n-1;i++){
            sum=sum+arr[i];
        
        }
        
         int s = (n*(n+1))/2;
        System.out.print("Missing value:" +s-sum);
        
        
} 
    }
    
