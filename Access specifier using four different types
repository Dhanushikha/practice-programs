import java.util.*;
class Main {
    public static void addTwoNumbers(){
        int result = a+b;
        System.out.println(result);
        return;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        addTwoNumbers(a,b);
        
    }
}
