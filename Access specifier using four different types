import java.util.*;
class Main {
    public static void  addTwoNumbers(){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int result =a+b;
        System.out.println(result);
    }
    public static void main(String[] args) {
         addTwoNumbers();
     
        
    }
}
