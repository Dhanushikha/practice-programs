import java.util.*;
class Main {
    public static int  addTwoNumbers(){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        return a+b;
    }
    public static void main(String[] args) {
        int result = addTwoNumbers();
        System.out.println(result);
        
    }
}
