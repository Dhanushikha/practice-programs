import java.lang.*;
import java.util.*;
public class Main {
    public static void main(String[] args) {
        StringBuffer sb = new StringBuffer("Hyderabad");
        for (int i =sb.length()-1; i>=0; i--) {
            if(i%2!=0){
                sb.deleteCharAt(i);
            }
        }
        System.out.println(sb);
    }
}
///
import java.lang.*;
import java.util.*;
public class Main {
    public static void main(String[] args) {
        StringBuffer sb = new StringBuffer("Hyderabad");
        for (int i =1; i<sb.length(); i++) {
                sb.deleteCharAt(i);
        }
        System.out.println(sb);
    }
}
