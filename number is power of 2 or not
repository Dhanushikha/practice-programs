import java.util.*;
class Main {
    public static void main(String[] args) {
    
        Scanner sc = new Scanner(System.in);
        int T = sc.nextInt();
        
        if(T>0 && (T & (T-1))==0)
        {
           System.out.println("YES"); 
        }
        else{
           System.out.println("NO"); 
        }
        }
    }
